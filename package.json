    "helmet": "^4.6.0",
    "moment-timezone": "^0.5.34",
    "mongodb-memory-server": "^8.7.2",
    "mongoose": "^6.4.5",
    "nanoid": "^3.1.30",
    "nestjs-stripe": "^1.0.0",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
