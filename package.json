    "@nestjs/platform-express": "^9.0.4",
    "@nestjs/platform-socket.io": "^9.1.4",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/websockets": "^9.1.4",
    "aws-sdk": "^2.1177.0",
    "bcrypt": "^5.0.1",
